Binding: CTUtemplate
Title: CTU Project Template
OpenFiles:
  INDEX
  03_*/00_MASTERFILE.R

Parameter: projNum
Widget: TextInput
Label: Project number
Default: 100

Parameter: projName
Widget: TextInput
Label: Project name
Default: Insert project name

Parameter: au
Widget: TextInput
Label: Author
Default: Insert name here

Parameter: stataFiles
Widget: CheckboxInput
Label: Create Stata files?
Default: On

Parameter: rFiles
Widget: CheckboxInput
Label: Create R files?
Default: On

